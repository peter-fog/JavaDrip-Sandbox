{
  "name": "sales-demo",
  "version": "5.1.16",
  "private": true,
  "engines": {
    "npm": ">=8.19.0",
    "node": ">=18.12.0",
    "yarn": "please-use-npm",
    "pnpm": "please-use-npm"
  },
  "scripts": {
    "dev": "run-s uniform:pull:manifest dev:next",
    "dev:next": "next dev --port 3000",
    "build": "run-s uniform:pull:manifest build:next",
    "build:next": "next build",
    "build:test": "export NEXT_PUBLIC_E2E_TEST=true && run-s build",
    "start": "next start",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,scss}\"",
    "uniform:pull:manifest": "uniform context manifest download --output ./src/context/manifest.json",
    "uniform:pull:prompts": "uniform canvas prompt pull ./content/prompts",
    "uniform:pull": "uniform sync pull",
    "uniform:push:prompts": "uniform canvas prompt push ./content/prompts",
    "uniform:entry:publish": "uniform canvas entry push ./content/entry --state published",
    "uniform:manifest:publish": "uniform context manifest publish",
    "uniform:publish": "run-s uniform:entry:publish uniform:manifest:publish",
    "import:products": "ts-node src/scripts/import-products",
    "uniform:get:redirects": "ts-node src/scripts/get-redirects.ts",
    "uniform:push": "uniform sync push",
    "postinstall": "cd cli-run && npm install && npm run build",
    "cli": "npm i && cd cli-run && node dist/index.mjs"
  },
  "dependencies": {
    "@contentful/rich-text-html-renderer": "^16.1.0",
    "@contentful/rich-text-types": "^16.2.0",
    "@segment/snippet": "4.16.2",
    "@supabase/supabase-js": "2.38.2",
    "@uniformdev/canvas": "19.91.0",
    "@uniformdev/canvas-next": "19.91.0",
    "@uniformdev/canvas-react": "19.91.0",
    "@uniformdev/context": "19.91.0",
    "@uniformdev/context-edge-vercel": "19.91.0",
    "@uniformdev/context-gtag": "19.91.0",
    "@uniformdev/context-next": "19.91.0",
    "@uniformdev/context-react": "19.91.0",
    "@uniformdev/project-map": "19.91.0",
    "@uniformdev/redirect": "19.91.0",
    "@uniformdev/richtext": "19.91.0",
    "@vercel/og": "^0.5.7",
    "algoliasearch": "4.18.0",
    "axios": "1.4.0",
    "classnames": "^2.3.2",
    "cookie": "^0.5.0",
    "daisyui": "^3.9.3",
    "framer-motion": "10.16.2",
    "next": "13.5.6",
    "react": "18.2.0",
    "react-cookie": "^4.1.1",
    "react-dom": "18.2.0",
    "react-instantsearch-hooks-web": "6.45.0",
    "react-multi-carousel": "^2.8.3",
    "react-player": "^2.12.0",
    "react-responsive-masonry": "2.1.7",
    "sass": "1.64.1",
    "sharp": "^0.32.1",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@algolia/recommend": "4.18.0",
    "@tailwindcss/typography": "^0.5.9",
    "@types/color": "^3.0.3",
    "@types/node": "^18.15.11",
    "@types/node-fetch": "^2.6.4",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18",
    "@types/react-responsive-masonry": "2.1.0",
    "@types/segment-analytics": "0.0.37",
    "@types/uuid": "9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@typescript-eslint/types": "^6.4.0",
    "@uniformdev/cli": "19.91.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.47.0",
    "eslint-config-custom": "*",
    "eslint-config-next": "^13.5.6",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.2",
    "shared": "*",
    "tailwind-config": "*",
    "tailwindcss": "^3.3.1",
    "ts-node": "^10.9.1",
    "typescript": "5.3.3"
  }
}
